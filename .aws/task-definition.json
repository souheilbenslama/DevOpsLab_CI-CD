{
    "family": "petsi-task-def-pipeline",
    "containerDefinitions": [
        {
            "name": "petsi-node-cont",
            "image": "souheilbenslama/petsi_node",
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp"
                }
            ]
           
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "memory": "1024",
    "cpu": "512"
}